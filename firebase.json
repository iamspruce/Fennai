{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": [
    {
      "codebase": "main",
      "source": "functions",
      "runtime": "python311"
    },
    {
      "codebase": "proxy",
      "source": "functions/proxy",
      "runtime": "python311"
    }
  ],
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8081
    },
    "storage": {
      "port": 9199
    },
    "functions": {
      "port": 5005
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
